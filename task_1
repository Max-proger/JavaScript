var a = 1, b = 1, c, d;
c = ++a; alert(c);           // 2 префиксная форма инкрементирования
d = b++; alert(d);           // 1 постфиксная форма инкрементирования
c = (2 + ++a); alert(c);      // 5 префиксная форма инкрементирования
d = (2 + b++); alert(d);      // 4 постфиксная форма инкрементирования
alert(a);                    // 3 операнд инкрементировался несколько раз
alert(b);                    // 3 операнд инкрементировался несколько раз
